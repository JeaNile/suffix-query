{  "name": "jeanile/suffix-query",  "type": "library",  "license": "MIT",  "keywords": [    "php",    "hyperf"  ],  "description": "Suffix Query Component",  "autoload": {    "psr-4": {      "Jeanile\\SuffixQuery\\": "src/"    }  },  "autoload-dev": {    "psr-4": {      "Jeanile\\SuffixQuery\\": "tests/"    }  },  "authors": [    {      "name": "JeaNile",      "email": "q962351668@gmail.com"    }  ],  "require": {    "php": "^7.4 || ^8.0",    "hyperf/contract": "~2.2.0",    "hyperf/utils": "~2.2.0",    "psr/container": "^1.0|^2.0"  },  "require-dev": {    "friendsofphp/php-cs-fixer": "^3.0",    "mockery/mockery": "^1.0",    "phpstan/phpstan": "^1.0",    "phpunit/phpunit": ">=7.0",    "swoole/ide-helper": "^4.5"  },  "scripts": {    "test": "phpunit -c phpunit.xml --colors=always",    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",    "cs-fix": "php-cs-fixer fix $1"  },  "extra": {    "hyperf": {      "config": "Jeanile\\SuffixQuery\\ConfigProvider"    }  }}